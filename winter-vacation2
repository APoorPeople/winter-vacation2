#include<stdio.h>
int main()
{
    int num;
    int count=0;
    int a[5];
    int b=0;
	int d;
	int c;
    scanf("%d",&num);
    while(num%10!=0)
    {
        count+=1;
        a[b]=num%10;
        num/=10;
        b++;
    }
    printf("%d\n",count);
    for(d=4;d>=1;d--)
    {
        printf("%d ",a[d]);
    }
	printf("%d",a[0]);
	printf("\n");
    for(c=0;c<5;c++)
    {
        printf("%d",a[c]);
    }


    return 0;
}
